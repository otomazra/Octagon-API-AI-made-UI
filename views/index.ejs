<%- include("partials/header.ejs"); %>
    <section>
        <h1 class="divisions-title">Divisions</h1>
        <% data.forEach(item=>{ %>
            <div data-id="<%= item.id%>" class="category-division-list">
                <h3 class="division-title"><%= item.categoryName%></h3>
                <div class="champ-division-list" data-id="<%= item.champion.id %>">
                    <h4 class="champ-title" data-id="<%= item.champion.id %>"><a href="/fighter/<%= item.champion.id %>">Champion: <span class="championName"><%= item.champion.championName %></span></a></h4>
                    <a href="/fighter/<%= item.champion.id %>"><img class="champImage-division-list" data-id="<%= item.champion.id %>" src="" alt=""></a>
                </div>
            </div>
        <% }) %>
    </section>

<%- include("partials/footer.ejs") %>